    "react-dom": "^18.2.0",
    "react-router-dom": "^6.17.0"
  },
  "devDependencies": {
    "@jinpro/eslint-config": "workspace:*",
    "@jinpro/tailwind-config": "workspace:*",
    "@jinpro/vite-config": "workspace:*",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "vite": "^4.5.0"
  }
